{
    "name": "ai-cv-evaluator",
    "version": "1.0.0",
    "description": "AI-powered CV and project evaluation service with LLM chaining and RAG",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js",
        "test": "jest --coverage",
        "test:watch": "jest --watch",
        "test:integration": "jest --testPathPattern=integration",
        "test:unit": "jest --testPathPattern=unit"
    },
    "keywords": [
        "ai",
        "llm",
        "cv-evaluation",
        "rag",
        "nodejs",
        "backend",
        "express"
    ],
    "author": "Muh Reza Aldi Irawan",
    "license": "MIT",
    "dependencies": {
        "cors": "^2.8.5",
        "dotenv": "^16.6.1",
        "express": "^4.18.2",
        "multer": "^1.4.5-lts.1",
        "openai": "^5.23.1",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "jest": "^29.7.0",
        "nodemon": "^3.0.1",
        "supertest": "^6.3.3"
    },
    "jest": {
        "testEnvironment": "node",
        "coverageDirectory": "coverage",
        "collectCoverageFrom": [
            "**/*.js",
            "!node_modules/**",
            "!coverage/**",
            "!uploads/**"
        ]
    },
    "engines": {
        "node": ">=16.0.0"
    }
}
